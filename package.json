{
  "name": "claytonerrington-website",
  "version": "1.0.0",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "concurrently": "^8.0.1",
    "eleventy-plugin-edit-on-github": "^1.1.0",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "netlify-plugin-cache-folder": "^0.0.8",
    "node-fetch": "2.6.1",
    "plop": "^3.1.2",
    "stylus": "^0.59.0"
  },
  "scripts": {
    "build:stylus": "stylus --compress ./src/stylus/ --out ./static/css/",
    "watch:stylus": "stylus --watch ./src/stylus/ --out ./static/css/",
    "build:11ty": "NODE_ENV=production npx @11ty/eleventy",
    "watch:11ty": "eleventy --serve",
    "start": "concurrently -n stylus,11ty \"npm:watch:stylus\"  \"netlify dev\"",
    "build": "npm run build:stylus && npm run build:11ty",
    "clean": "node ./tools/clean.js",
    "dev": "concurrently -n stylus,11ty \"npm:watch:stylus\" \"npm:watch:11ty\"",
    "newpost": "plop",
    "git": "git add . && git commit -m",
    "postgit": "git push --all"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "eleventy-plugin-seo": "^0.5.2",
    "npm-check-updates": "^16.10.12"
  }
}
