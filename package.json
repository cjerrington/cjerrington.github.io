    "netlify-plugin-cache": "^1.0.3",
    "normalize-url": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-import-ext-glob": "^2.1.1",
    "postcss-js": "^4.0.1",
    "prettier-plugin-jinja-template": "^1.5.0",
    "rimraf": "^6.0.1",
    "rss-parser": "^3.13.0",
    "sanitize-html": "^2.13.1",
    "slugify": "^1.6.6"
