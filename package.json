{
  "name": "claytonerrington-website",
  "version": "2.2.2",
  "description": "Personal website built with Eleventy to host the topics I blog about",
  "author": "Clayton Errington",
  "license": "ISC",
  "engines": {
    "node": ">=20.x.x"
  },
  "scripts": {
    "clean": "rimraf dist",
    "envcheck": "node getenv.js",
    "dev:11ty": "eleventy --serve --watch",
    "build:11ty": "npx @11ty/eleventy",
    "start": "run-p envcheck dev:*",
    "build": "run-s envcheck clean build:*",
    "newpost": "plop --plopfile=plopfile.js",
    "newimage": "plop --plopfile=the100pics.js"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/cjerrington/cjerrington.github.io.git",
    "name": "cjerrington/cjerrington.github.io"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-fetch": "5.0.1",
    "@11ty/eleventy-img": "^4.0.2",
    "@11ty/eleventy-plugin-bundle": "^1.0.4",
    "@11ty/eleventy-plugin-inclusive-language": "^1.0.3",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@11tyrocks/eleventy-plugin-emoji-readtime": "^1.0.1",
    "@mikestreety/11ty-utils": "^1.3.0",
    "@rknightuk/eleventy-plugin-post-graph": "^1.0.8",
    "@umami/node": "^0.4.0",
    "@zachleat/snow-fall": "^1.0.3",
    "eleventy-plugin-reader-bar": "^0.2.1",
    "eleventy-plugin-reading-time": "^0.0.1",
    "tailwindcss": "^3.4.15"
  },
  "devDependencies": {
    "@csstools/postcss-relative-color-syntax": "^2.0.19",
    "@toycode/markdown-it-class": "^1.2.4",
    "autoprefixer": "^10.4.20",
    "clean-css": "^5.3.3",
    "cssnano": "^7.0.6",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "esbuild": "^0.24.0",
    "html-minifier-terser": "^7.2.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "markdown-it-abbr": "^2.0.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-eleventy-img": "^0.10.2",
